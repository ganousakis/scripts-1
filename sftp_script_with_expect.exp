#!/usr/bin/expect
#https://docs.oracle.com/cd/E35328_01/E35336/html/vmcli-script.html

spawn sftp -oIdentityFile=$HOME/.ssh/my_ssh_key USER@SERVER
expect "USER@SERVER\'s password:" 
send "MY_PASSWORD\r"
expect "sftp>"
send "lcd local/path\r"
expect "sftp>"
send "get *\r"
expect "sftp>"
send "bye\r"
interact
